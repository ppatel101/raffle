<h2>Sign up</h2>

<%= form_with scope: :user, url: users_path, local: true do |form| %>
    <p>
      <%= form.label :email %><br>
      <%= form.text_field :email %>
    </p>

    <p>
      <%= form.label :email %><br>
      <%= form.collection_select :type_id, Type.all, :id, :name %>
    </p>
   
    <p>
      <%= form.submit %>
    </p>
  <% end %>